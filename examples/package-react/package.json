{
  "name": "@myorg/example-react",
  "version": "0.0.1",
  "description": "A React component library package",
  "type": "module",
  "license": "MIT",
  "author": "Resona <contact@myorg.art>",
  "homepage": "https://github.com/electroaudiogram/resona#readme",
  "repository": "electroaudiogram/resona.git",
  "bugs": "https://github.com/electroaudiogram/resona/issues",
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./button": "./src/Button.tsx"
  },
  "publishConfig": {
    "exports": {
      "./button": {
        "import": "./dist/Button.js",
        "require": "./dist/Button.cjs",
        "types": "./dist/Button.d.ts"
      },
      ".": {
        "import": "./dist/*.js",
        "require": "./dist/*.cjs",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage,tsconfig.tsbuildinfo}",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "4.0.17",
    "eslint-config-react": "workspace:*",
    "jsdom": "27.4.0",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "typescript-config": "workspace:*",
    "vitest": "4.0.16"
  }
}
