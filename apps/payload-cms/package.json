{
  "name": "payload-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "next build --webpack",
    "clean": "shx rm -rf {node_modules,.turbo,.next,next-env.d.ts,coverage,storybook-static}",
    "dev": "next dev --webpack",
    "dev:ci": "next dev --webpack",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "paraglide": "pnpm dlx @inlang/paraglide-js compile --project ../../.inlang --outdir ./src/i18n",
    "postinstall": "pnpm paraglide && pnpm exec playwright install --with-deps",
    "predev": "docker compose up -d && payload seed",
    "seed": "payload seed",
    "start": "next start",
    "test": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "test:int": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/translations": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "dotenv": "17.2.3",
    "graphql": "16.12.0",
    "next": "16.1.1",
    "payload": "3.69.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "2.7.1",
    "@myorg/eslint-config-next": "workspace:*",
    "@myorg/tailwind-config": "workspace:*",
    "@myorg/typescript-config": "workspace:*",
    "@playwright/test": "1.57.0",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "jsdom": "27.4.0",
    "playwright": "1.57.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}
