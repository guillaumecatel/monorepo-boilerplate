{
  "name": "@myorg/shared",
  "version": "0.0.2",
  "description": "Utilities for projects",
  "type": "module",
  "license": "MIT",
  "private": false,
  "author": "MyOrg Team <contact@myorg.com>",
  "homepage": "https://github.com/myorg/myrepo#readme",
  "repository": "myorg/myrepo",
  "bugs": "https://github.com/myorg/myrepo/issues",
  "keywords": [
    "utilities",
    "helpers",
    "tools"
  ],
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./alias": "./src/alias.ts",
    "./guards": "./src/guards.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage,tsconfig.tsbuildinfo}",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.16",
    "eslint-config": "workspace:*",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "typescript-config": "workspace:*",
    "vitest": "4.0.16"
  }
}
