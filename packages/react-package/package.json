{
  "name": "@myorg/react-package",
  "version": "0.0.1",
  "description": "False",
  "type": "module",
  "license": "MIT",
  "author": "My Org <contact@myorg.com>",
  "homepage": "https://github.com/myorg/my-repo#readme",
  "repository": "myorg/my-repo.git",
  "bugs": "https://github.com/myorg/my-repo/issues",
  "keywords": [
    "false"
  ],
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./button": "./src/Button.tsx"
  },
  "publishConfig": {
    "exports": {
      "./button": {
        "import": "./dist/Button.js",
        "require": "./dist/Button.cjs",
        "types": "./dist/Button.d.ts"
      },
      ".": {
        "import": "./dist/*.js",
        "require": "./dist/*.cjs",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@myorg/eslint-config": "workspace:*",
    "@myorg/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "4.0.16",
    "jsdom": "27.4.0",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
