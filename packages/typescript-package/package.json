{
  "name": "@myorg/typescript-package",
  "version": "0.0.1",
  "description": "False",
  "type": "module",
  "license": "MIT",
  "author": "My Org <contact@myorg.com>",
  "homepage": "https://github.com/myorg/my-repo#readme",
  "repository": "myorg/my-repo.git",
  "bugs": "https://github.com/myorg/my-repo/issues",
  "keywords": [
    "false"
  ],
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./alias": "./src/alias.ts",
    "./guards": "./src/guards.ts"
  },
  "publishConfig": {
    "exports": {
      "./alias": {
        "import": "./dist/alias.js",
        "require": "./dist/alias.cjs",
        "types": "./dist/alias.d.ts"
      },
      "./guards": {
        "import": "./dist/guards.js",
        "require": "./dist/guards.cjs",
        "types": "./dist/guards.d.ts"
      },
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@myorg/eslint-config": "workspace:*",
    "@myorg/typescript-config": "workspace:*",
    "@vitest/coverage-v8": "4.0.16",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
