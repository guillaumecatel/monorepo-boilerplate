{
  "name": "monorepo",
  "description": "",
  "type": "module",
  "private": true,
  "author": "",
  "packageManager": "pnpm@10.0.0",
  "engineStrict": true,
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "turbo build --filter=!./examples/**",
    "build:apps": "turbo build --filter=./apps/**",
    "build:examples": "turbo build --filter=./examples/**",
    "build:packages": "turbo build --filter=./packages/**",
    "clean": "turbo clean",
    "cli": "turbo gen",
    "commitlint": "commitlint",
    "dev": "turbo dev --filter=!./examples/**",
    "dev:apps": "turbo dev --filter=./apps/**",
    "dev:examples": "turbo dev --filter=./examples/**",
    "dev:packages": "turbo dev --filter=./packages/**",
    "e2e": "turbo e2e --filter=!./examples/**",
    "e2e:apps": "turbo e2e --filter=./apps/**",
    "e2e:examples": "turbo e2e --filter=./examples/**",
    "e2e:packages": "turbo e2e --filter=./packages/**",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "knip": "knip --no-exit-code",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "nuke": "pnpm clean && shx rm -rf {node_modules,.turbo}",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "syncpack": "syncpack",
    "syncpack:fix": "syncpack format && syncpack fix-mismatches",
    "test": "turbo test --filter=!./examples/**",
    "test:apps": "turbo test --filter=./apps/**",
    "test:examples": "turbo test --filter=./examples/**",
    "test:packages": "turbo test --filter=./packages/**",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.0",
    "@commitlint/config-conventional": "20.3.0",
    "@turbo/gen": "2.7.4",
    "@types/node": "25.0.3",
    "eslint": "9.39.2",
    "eslint-config-all": "workspace:*",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "knip": "5.78.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-sh": "0.18.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "shx": "0.4.0",
    "syncpack": "13.0.4",
    "turbo": "2.7.4",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "diff@<8.0.3": ">=8.0.3",
      "undici@>=7.0.0 <7.18.2": ">=7.18.2",
      "devalue@>=5.3.0 <=5.6.1": ">=5.6.2",
      "h3@<=1.15.4": ">=1.15.5"
    }
  }
}
